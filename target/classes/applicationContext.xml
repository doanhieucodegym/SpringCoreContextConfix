<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="beanA" class="com.hivetech.springcore.xml.bean.A"/>
    <bean id="beanB" class="com.hivetech.springcore.xml.bean.B"/>
    <bean id="beanC" class="com.hivetech.springcore.xml.bean.C">
        <property name="instanceA" ref="beanA"/>
        <property name="instanceB" ref="beanB"/>
    </bean>

</beans>